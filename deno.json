{
  "$schema": "https://deno.land/x/deno/cli/schemas/config-file.v1.json",
  "name": "@flowcore/time-uuid",
  "description": "Utils for working with time-based UUIDs",
  "version": "0.1.2",
  "license": "Apache-2.0",
  "exports": "./src/mod.ts",
  "publish": {
    "include": [
      "src/**/*.ts",
      "README.md",
      "CHANGELOG.md"
    ]
  },
  "tasks": {},
  "imports": {
    "@deno/dnt": "jsr:@deno/dnt@^0.41.3",
    "long": "npm:long@^5.3.1"
  },
  "fmt": {
    "exclude": [
      "node_modules",
      "npm"
    ],
    "lineWidth": 120,
    "indentWidth": 2,
    "useTabs": false,
    "semiColons": false
  },
  "lint": {
    "exclude": [
      "node_modules",
      "npm"
    ]
  }
}
